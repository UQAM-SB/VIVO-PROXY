
<settings>
    <profiles>
        <profile>
            <id>localhost-jetty</id>
            <properties>
                <skipTests>true</skipTests>
                <skipJetty>false</skipJetty>
                <webapp.depl>webapp-localhost-jetty</webapp.depl>
            </properties>
        </profile>
        <profile>
            <id>localhost</id>
            <properties>
                <skipTests>true</skipTests>
                <skipJetty>true</skipJetty>
                <webapp.depl>webapp-localhost-tomcat</webapp.depl>
            </properties>
        </profile>
        <profile>
            <id>aws-dev</id>
            <properties>
                <skipTests>true</skipTests>
                <skipJetty>true</skipJetty>
                <aws.skipCopy>false</aws.skipCopy> 
                <webapp.target>/var/lib/tomcat/webapps/ROOT</webapp.target>
                <webapp.depl>webapp-aws-dev</webapp.depl>
            </properties>
        </profile>
        <profile>
            <id>aws-dev-local</id>
            <properties>
                <skipTests>true</skipTests>
                <skipJetty>true</skipJetty>
                <aws.skipCopy>false</aws.skipCopy> 
                <webapp.target>${project.basedir}/../../releng/ca.uqam.tool.vivo-proxy.beanstalk/aws-deploy-from-local</webapp.target>
                <webapp.depl>webapp-aws-dev</webapp.depl>
            </properties>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>localhost-jetty</activeProfile>
    </activeProfiles>
</settings>
